{
  "name": "medusa-file-gcp",
  "version": "0.0.3",
  "description": "Google cloud storage plugin for medusajs",
  "main": "index.js",
  "author": "Glenford Williams",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drewdecarme/medusa-file-cloudflare-images"
  },
  "keywords": [
    "medusa",
    "gcp",
    "google cloud",
    "filestorage",
    "medusa-plugin"
  ],
  "scripts": {
    "bundle": "esbuild src/services/** --outdir=services --format=cjs --target=esnext",
    "bundle-swc": "swc src -d .",
    "build": "yarn bundle",
    "build:ci": "yarn bundle",
    "prepare": "yarn build",
    "dev": "yarn bundle --watch",
    "test": "jest",
    "package": "yarn build && yarn np --no-tests --no-yarn"
  },
  "peerDependencies": {
    "medusa-interfaces": "1.x"
  },
  "files": [
    "services"
  ],
  "devDependencies": {
    "@types/formidable": "^2.0.3",
    "@types/nanoid": "^3.0.0",
    "chokidar": "^3.5.2",
    "client-sessions": "^0.8.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.10",
    "eslint": "^8.6.0",
    "jest": "^27.4.7",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.18.1",
    "body-parser": "^1.19.1",
    "express": "^4.17.2",
    "form-data": "^4.0.0",
    "formidable": "^2.0.1",
    "medusa-core-utils": "^1.1.31",
    "medusa-interfaces": "^1.1.34",
    "medusa-test-utils": "^1.1.36",
    "nanoid": "^2.1.11",
    "node-fetch": "2.6.6",
    "np": "^7.6.0",
    "regenerator-runtime": "^0.13.9",
    "typeorm": "^0.2.41"
  },
  "packageManager": "yarn@3.1.1"
}
